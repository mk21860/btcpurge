extends layout

block content
    center
    .codrops-top.clearfix
      a.codrops-icon.codrops-icon-prev(href='http://tympanus.net/Tutorials/AutomaticFigureNumbering/')
        span Previous Demo
      span.right
        a.codrops-icon.codrops-icon-drop(href='http://tympanus.net/codrops/?p=15001')
          span Back to the Codrops Article
    .pt-triggers
      button#iterateEffects.pt-touch-button Show next page transition
      #dl-menu.dl-menuwrapper
        button.dl-trigger Choose a transition
        ul.dl-menu
          li
            a(href='#') Move
            ul.dl-submenu
              li(data-animation='1')
                a(href='#') Move to left/ from right
              li(data-animation='2')
                a(href='#') Move to right/ from left
              li(data-animation='3')
                a(href='#') Move to top/ from bottom
              li(data-animation='4')
                a(href='#') Move to bottom/ from top
          li
            a(href='#') Fade
            ul.dl-submenu
              li(data-animation='5')
                a(href='#') Fade / from right
              li(data-animation='6')
                a(href='#') Fade / from left
              li(data-animation='7')
                a(href='#') Fade / from bottom
              li(data-animation='8')
                a(href='#') Fade / from top
              li(data-animation='9')
                a(href='#') Fade left / Fade right
              li(data-animation='10')
                a(href='#') Fade right / Fade left
              li(data-animation='11')
                a(href='#') Fade top / Fade bottom
              li(data-animation='12')
                a(href='#') Fade bottom / Fade top
          li
            a(href='#') Different easing
            ul.dl-submenu
              li(data-animation='13')
                a(href='#') Different easing / from right
              li(data-animation='14')
                a(href='#') Different easing / from left
              li(data-animation='15')
                a(href='#') Different easing / from bottom
              li(data-animation='16')
                a(href='#') Different easing / from top
          li
            a(href='#') Scale
            ul.dl-submenu
              li(data-animation='17')
                a(href='#') Scale down / from right
              li(data-animation='18')
                a(href='#') Scale down / from left
              li(data-animation='19')
                a(href='#') Scale down / from bottom
              li(data-animation='20')
                a(href='#') Scale down / from top
              li(data-animation='21')
                a(href='#') Scale down / scale down
              li(data-animation='22')
                a(href='#') Scale up / scale up
              li(data-animation='23')
                a(href='#') Move to left / scale up
              li(data-animation='24')
                a(href='#') Move to right / scale up
              li(data-animation='25')
                a(href='#') Move to top / scale up
              li(data-animation='26')
                a(href='#') Move to bottom / scale up
              li(data-animation='27')
                a(href='#') Scale down / scale up
          li
            a(href='#') Rotate
            ul.dl-submenu
              li
                a(href='#') Glue
                ul.dl-submenu
                  li(data-animation='28')
                    a(href='#') Glue left / from right
                  li(data-animation='29')
                    a(href='#') Glue right / from left
                  li(data-animation='30')
                    a(href='#') Glue bottom / from top
                  li(data-animation='31')
                    a(href='#') Glue top / from bottom
              li
                a(href='#') Flip
                ul.dl-submenu
                  li(data-animation='32')
                    a(href='#') Flip right
                  li(data-animation='33')
                    a(href='#') Flip left
                  li(data-animation='34')
                    a(href='#') Flip top
                  li(data-animation='35')
                    a(href='#') Flip bottom
              li(data-animation='36')
                a(href='#') Fall
              li(data-animation='37')
                a(href='#') Newspaper
              li
                a(href='#') Push / Pull
                ul.dl-submenu
                  li(data-animation='38')
                    a(href='#') Push left / from right
                  li(data-animation='39')
                    a(href='#') Push right / from left
                  li(data-animation='40')
                    a(href='#') Push top / from bottom
                  li(data-animation='41')
                    a(href='#') Push bottom / from top
                  li(data-animation='42')
                    a(href='#') Push left / pull right
                  li(data-animation='43')
                    a(href='#') Push right / pull left
                  li(data-animation='44')
                    a(href='#') Push top / pull bottom
                  li(data-animation='45')
                    a(href='#') Push bottom / pull top
              li
                a(href='#') Fold / Unfold
                ul.dl-submenu
                  li(data-animation='46')
                    a(href='#') Fold left / from right
                  li(data-animation='47')
                    a(href='#') Fold right / from left
                  li(data-animation='48')
                    a(href='#') Fold top / from bottom
                  li(data-animation='49')
                    a(href='#') Fold bottom / from top
                  li(data-animation='50')
                    a(href='#') Move to right / unfold left
                  li(data-animation='51')
                    a(href='#') Move to left / unfold right
                  li(data-animation='52')
                    a(href='#') Move to bottom / unfold top
                  li(data-animation='53')
